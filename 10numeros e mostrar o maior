#include <stdio.h>
#include<stdlib.h>


 
  int main (){
  	 int n1[10];
  	int i,aux=0;
  	
  	for(i=0;i<=9;i++)
  	{
  		printf("Entre com o %d numero do vetor",i+1);
  		scanf("%d",&n1[i]);	
  		if(n1[i]>aux)
  		aux = n1[i];
}


printf("O maior valor eh %d",aux);




  	
  	system("pause");
  	return 0;
  	
  	
  	
  	
  	
  	
  	
  }
